PREFIX : <http://a.example/cds#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ucum: <http://unitsofmeasure.org/>
IMPORT <patientDB.shex>
BASE <http://a.example/driver>

<#Lab> {
  :labCode IRI ;
  :subject @<#Patient> ;
  :collectionDate @<#Date> ;
  :result @<#Numeric> OR LITERAL ;
  :time xsd:dateTime ;
}

<#Date> xsd:dateTime OR xsd:date OR xsd:year

<#Numeric> xsd:integer OR xsd:double

